<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="si.xlab.research.emmy.demo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="si.xlab.research.emmy.demo.activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="si.xlab.research.emmy.demo.activities.SettingsActivity"
            android:label="@string/title_activity_settings" />
        <activity android:name=".activities.AnonymousCredentialActivity" />
        <activity android:name="si.xlab.research.emmy.demo.activities.AnonymousCredentialActivity" />
        <activity android:name="si.xlab.research.emmy.demo.activities.GetCredentialActivity" />
        <activity android:name="si.xlab.research.emmy.demo.activities.ExampleActivity" />
    </application>

</manifest>
